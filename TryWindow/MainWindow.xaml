<Window x:Class="TryWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TryWindow"
        mc:Ignorable="d"
        Title="Expression Evaluate" Width="400" SizeToContent="Height">
    <StackPanel>
        <TextBox x:Name="ScriptTextBox" />
        <Button x:Name="CalculateButton" Content="_Calculate" IsDefault="True" Click="CalculateButton_Click" />
        <TextBlock x:Name="ResultTextBlock" />
    </StackPanel>
</Window>
